<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap>
	<typeAlias alias="gyest" type="com.hanbit.dto.GuestDto"/>
	
	<insert  id="add" parameterClass="com.hanbit.dto.GuestDto">
		insert into guest values(#sabun#, #name#, sysdate, #pay#)
	</insert>

	<select id="selectAll"  resultClass="insa">
		select  * from guest order by sabun 
	</select>		
	<select id="detail" resultClass="guest">
		select * from gueset where sabun = #value#
	</select>
	
	
</sqlMap>

















